CREATE TABLE YMF.TBL_OPERATE_RECODE (
  ID          BIGINT PRIMARY KEY GENERATED BY DEFAULT AS IDENTITY,
  MEMBER_NO   VARCHAR(50), --易宝会员号
  BIZ_TYPE    VARCHAR(20), --业务类型
  BIZ_PARAM   VARCHAR(50), --业务参数
  COUNT       INT, --次数
  LINK_NAME   VARCHAR(50), --联系人
  LINK_PHONE  VARCHAR(64), --电话
  PROVINCE_NAME VARCHAR(100), --省
  CITY_NAME   VARCHAR(100), --市
  REMARK      VARCHAR(100), --备注
  CREATE_TIME TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
  UPDATE_TIME TIMESTAMP --更新时间
);
COMMENT ON TABLE YMF.TBL_OPERATE_RECODE IS '用户操作记录表';
CREATE INDEX YMF.OPERATE_RECODE_MEMBER_NO_IDX
  ON YMF.TBL_OPERATE_RECODE (MEMBER_NO);
CREATE INDEX YMF.OPERATE_RECODE_LINK_PHONE_IDX
  ON YMF.TBL_OPERATE_RECODE (LINK_PHONE);
CREATE INDEX YMF.OPERATE_RECODE_CREATE_TIME_IDX
  ON YMF.TBL_OPERATE_RECODE (CREATE_TIME);
GRANT INSERT, UPDATE, DELETE, SELECT ON TABLE YMF.TBL_OPERATE_RECODE TO USER YMFAPP;
GRANT SELECT ON TABLE YMF.TBL_OPERATE_RECODE TO USER YMFREAD;
GRANT SELECT ON TABLE YMF.TBL_OPERATE_RECODE TO USER QUERY;