    public void setMerchantAppType(String merchantAppType) {
        this.merchantAppType = merchantAppType;
    }

    public String getFeeChargeType() {
        return feeChargeType;
    }

    public void setFeeChargeType(String feeChargeType) {
        this.feeChargeType = feeChargeType;
    }

    public String getRiskMargin() {
        return riskMargin;
    }

    public void setRiskMargin(String riskMargin) {
        this.riskMargin = riskMargin;
    }

