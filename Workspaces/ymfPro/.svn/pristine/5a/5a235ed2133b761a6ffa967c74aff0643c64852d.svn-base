    /**
     * 开通产品
     */
    private List<String> openProductList;

    /**
     * 结算方式
     */
    private String settleMethod = "PERIOD";

    /**
     * 结算周期
     */
    private String settleCycle = "WEEK";

    /**
     * 结算日期
     */
    private String settleDate = "1,2,3,4,5,6,7";

    /**
     * 最小起结金额
     */
    private String minSettleAmount = "10";

    /**
     * 是否有服务号
     */
    private boolean hasServiceNo = false;

    /**
     * 费率信息
     */
    private Map<String, String> feeRateMap;

    /**
     * 限额信息
     */
    private Map<String, String> riskInfoMap;

    /**
     * 制式协议
     */
    private boolean standardAgreement = false;

    /**
