package com.yeepay.g3.facade.laike.dto.alliance;

import com.yeepay.g3.facade.laike.dto.BaseResponse;

import java.math.BigDecimal;

/**
 * Description: 展业统计
 * Author: wei.li
 * Date: 17/5/25
 * Version: 1.0
 * Copyright © 2016 YeePay.com All rights reserved.
 */
public class StatisticsResponse extends BaseResponse {

	/**
	 * 盟主
	 */
	private String lordName;

	/**
	 * 电话
	 */
	private String lordTel;

	/**
	 * 子商户总数
	 */
	private Integer childMerchant = 0;

	/**
	 * 联盟总数
	 */
	private Integer lolMerchant = 0;

	/**
	 * 盟友子商户总数
	 */
	private Integer lolChildMerchant = 0;

	/**
     * 商户月汇总
     */
	private BigDecimal childMonth = new BigDecimal(0);

	/**
     * 联盟月汇总
     */
	private BigDecimal lolMonth = new BigDecimal(0);

	public String getLordName() {
		return lordName;
	}

	public String getLordTel() {
		return lordTel;
	}

	public void setLordTel(String lordTel) {
		this.lordTel = lordTel;
	}

	public void setLordName(String lordName) {
		this.lordName = lordName;
	}

	public Integer getChildMerchant() {
		return childMerchant;
	}

	public void setChildMerchant(Integer childMerchant) {
		this.childMerchant = childMerchant;
	}

	public Integer getLolMerchant() {
		return lolMerchant;
	}

	public void setLolMerchant(Integer lolMerchant) {
		this.lolMerchant = lolMerchant;
	}

	public Integer getLolChildMerchant() {
		return lolChildMerchant;
	}

	public void setLolChildMerchant(Integer lolChildMerchant) {
		this.lolChildMerchant = lolChildMerchant;
	}

	public BigDecimal getChildMonth() {
		return childMonth;
	}

	public void setChildMonth(BigDecimal childMonth) {
		this.childMonth = childMonth;
	}

	public BigDecimal getLolMonth() {
		return lolMonth;
	}

	public void setLolMonth(BigDecimal lolMonth) {
		this.lolMonth = lolMonth;
	}
}
