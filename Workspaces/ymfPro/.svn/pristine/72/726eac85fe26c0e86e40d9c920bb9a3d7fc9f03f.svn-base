package com.yeepay.g3.facade.laike.exception;

/**
 * 错误码来源
 *
 * @author Created by Felix on 4/18/16.
 */
public enum ErrorCodeSource {

	LIKER("LK"),

	POS_AGENT("XXDLS"),

	CS_MERCHANT("SHRWXT"),

    YMF("YMF"),

	ALC("ALC"),

    CFL("CFL");

	/**
	 * 系统编码
	 */
	private final String sysCode;

	/**
	 * 构造方法
	 *
	 * @param sysCode
	 */
	ErrorCodeSource(String sysCode) {
		this.sysCode = sysCode;
	}

	public String getSysCode() {
		return sysCode;
	}
}
