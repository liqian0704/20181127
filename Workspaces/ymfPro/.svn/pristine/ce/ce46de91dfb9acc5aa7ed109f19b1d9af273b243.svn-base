package com.yeepay.g3.facade.laike.dto;

import com.yeepay.g3.facade.laike.enumtype.BizTypeEnum;

/**
 * Description: 硬件申请request
 * Author: wei.li
 * Date: 17/5/24
 * Version: 1.0
 * Copyright © 2016 YeePay.com All rights reserved.
 */
public class ApplyBizRequest extends BaseRequest {

    /**
     * 联系人
     */
    private String linkName;

    /**
     * 联系电话
     */
    private String linkPhone;

    /**
     * 硬件信息
     */
    private String deviceInfo;

    /**
     * 业务类型
     */
    private BizTypeEnum bizType;

    /**
     * 联系人省份
     */
    private String linkProvinceName;

    /**
     * 联系人市
     */
    private String linkMerCityName;

    /**
     * 备注
     */
    private String remark;

    public String getLinkName() {
        return linkName;
    }

    public void setLinkName(String linkName) {
        this.linkName = linkName;
    }

    public String getLinkPhone() {
        return linkPhone;
    }

    public void setLinkPhone(String linkPhone) {
        this.linkPhone = linkPhone;
    }

    public String getDeviceInfo() {
        return deviceInfo;
    }

    public void setDeviceInfo(String deviceInfo) {
        this.deviceInfo = deviceInfo;
    }

    public BizTypeEnum getBizType() {
        return bizType;
    }

    public void setBizType(BizTypeEnum bizType) {
        this.bizType = bizType;
    }

    public String getLinkProvinceName() {
        return linkProvinceName;
    }

    public void setLinkProvinceName(String linkProvinceName) {
        this.linkProvinceName = linkProvinceName;
    }

    public String getLinkMerCityName() {
        return linkMerCityName;
    }

    public void setLinkMerCityName(String linkMerCityName) {
        this.linkMerCityName = linkMerCityName;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}
