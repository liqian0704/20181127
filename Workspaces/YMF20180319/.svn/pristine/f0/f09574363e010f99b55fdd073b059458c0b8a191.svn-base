package com.yeepay.g3.facade.ymf.enumtype.sp;

/**
 * 消息通知类型
 * 使用:
 * 1. COD异步消息
 */
public enum NotifyType {

	MANUAL("回执发送"),
	AUTO("自动发送");

	String displayName;

	NotifyType(String displayName) {
		this.displayName = displayName;
	}

	public String getDisplayName() {
		return displayName;
	}
}
